{
  "id": "gio",
  "name": "gio",
  "services": [
    {
      "id": "frontend",
      "image": {
        "name": "gio-frontend-ms:latest",
        "local": true,
        "env": {
          "API_GATEWAY_HOST": "api-gateway",
          "API_GATEWAY_PORT": "30000"
        },
        "ports": [
          {
            "name": "gio-frontend-endpoint",
            "host_port": 5005,
            "container_port": 8080,
            "expose": 30005
          }
        ]
      },
      "t_proc": 2,
      "hw_reqs": 1,
      "iot_reqs": [],
      "sec_reqs": []
    },
    {
      "id": "api-gateway",
      "image": {
        "name": "gio-api-gateway-ms:latest",
        "local": true,
        "env": {
          "DEVICE_SERVICE_HOST": "device-ms",
          "DEVICE_SERVICE_PORT": "30001"
        },
        "ports": [
          {
            "name": "gio-api-gateway-endpoint",
            "host_port": 5000,
            "container_port": 8080,
            "expose": 30000
          }
        ]
      },
      "t_proc": 2,
      "hw_reqs": 1,
      "iot_reqs": [],
      "sec_reqs": []
    },
    {
      "id": "device-ms",
      "image": {
        "name": "gio-device-ms:latest",
        "local": true,
        "env": {
          "DEVICE_DRIVER_HOST": "device-driver",
          "DEVICE_DRIVER_PORT": "30006"
        },
        "ports": [
          {
            "name": "gio-device-ms-endpoint",
            "host_port": 5001,
            "container_port": 8080,
            "expose": 30001
          }
        ]
      },
      "t_proc": 2,
      "hw_reqs": 1,
      "iot_reqs": [],
      "sec_reqs": []
    },
    {
      "id": "device-driver",
      "image": {
        "name": "gio-device-driver:latest",
        "local": true,
        "env": {
          "FOG_NODE_HOST": "localhost",
          "FOG_NODE_PORT": "8080",
          "DEVICE_SERVICE_HOST": "device-ms",
          "DEVICE_SERVICE_PORT": "30001"
        },
        "ports": [
          {
            "name": "gio-device-driver-endpoint",
            "host_port": 5006,
            "container_port": 8080,
            "expose": 30006
          }
        ]
      },
      "t_proc": 2,
      "hw_reqs": 1,
      "iot_reqs": [
        "fognode"
      ],
      "sec_reqs": []
    }
  ],
  "flows": [
    {
      "src": "frontend",
      "dst": "api-gateway",
      "bandwidth": 1
    },
    {
      "src": "api-gateway",
      "dst": "device-ms",
      "bandwidth": 1
    },
    {
      "src": "device-ms",
      "dst": "device-driver",
      "bandwidth": 1
    }
  ],
  "max_latency": [
    {
      "chain": [
        "frontend",
        "api-gateway",
        "device-ms",
        "device-driver"
      ],
      "value": 99999
    }
  ]
}