{
  "id": "gio",
  "name": "gio",
  "services": [
    {
      "id": "frontend",
      "image": {
        "name": "gio-frontend-ms:latest",
        "local": true,
        "env": {
          "API_GATEWAY_HOST": "gio-api-gateway-endpoint",
          "API_GATEWAY_PORT": "5000"
        },
        "ports": [
          {
            "name": "gio-frontend-endpoint",
            "host_port": 5005,
            "container_port": 8080,
            "expose": 30005
          }
        ]
      },
      "t_proc": 2,
      "hw_reqs": 1,
      "iot_reqs": [],
      "sec_reqs": []
    },
    {
      "id": "api-gateway",
      "image": {
        "name": "gio-api-gateway-ms:latest",
        "local": true,
        "env": {
          "DEVICE_SERVICE_HOST": "gio-device-ms-endpoint",
          "DEVICE_SERVICE_PORT": "5001"
        },
        "ports": [
          {
            "name": "gio-api-gateway-endpoint",
            "host_port": 5000,
            "container_port": 8080,
            "expose": 30000
          }
        ]
      },
      "t_proc": 2,
      "hw_reqs": 1,
      "iot_reqs": [],
      "sec_reqs": []
    },
    {
      "id": "device-ms",
      "image": {
        "name": "gio-device-ms:latest",
        "local": true,
        "env": {
          "DEVICE_DRIVER_HOST": "gio-device-driver-endpoint",
          "DEVICE_DRIVER_PORT": "5006"
        },
        "ports": [
          {
            "name": "gio-device-ms-endpoint",
            "host_port": 5001,
            "container_port": 8080,
            "expose": 30001
          }
        ]
      },
      "t_proc": 2,
      "hw_reqs": 1,
      "iot_reqs": [],
      "sec_reqs": []
    },
    {
      "id": "device-driver-1",
      "image": {
        "name": "gio-device-driver:latest",
        "local": true,
        "env": {
          "FOG_NODE_HOST": "10.44.0.0",
          "FOG_NODE_PORT": "5003",
          "DEVICE_SERVICE_HOST": "gio-device-ms-endpoint",
          "DEVICE_SERVICE_PORT": "5001",
          "CALLBACK_HOST": "localhost",
          "CALLBACK_PORT": "5006"
        },
        "ports": [
          {
            "name": "gio-device-driver-1-endpoint",
            "host_port": 5006,
            "container_port": 8080,
            "expose": 30006
          }
        ]
      },
      "node_name": "k8s-node-1",
      "t_proc": 2,
      "hw_reqs": 1,
      "iot_reqs": [
        "FE:F4:1C:74:66:B3"
      ],
      "sec_reqs": []
    },
    {
      "id": "device-driver-2",
      "image": {
        "name": "gio-device-driver:latest",
        "local": true,
        "env": {
          "FOG_NODE_HOST": "10.44.0.0",
          "FOG_NODE_PORT": "5003",
          "DEVICE_SERVICE_HOST": "gio-device-ms-endpoint",
          "DEVICE_SERVICE_PORT": "5001",
          "CALLBACK_HOST": "localhost",
          "CALLBACK_PORT": "5006"
        },
        "ports": [
          {
            "name": "gio-device-driver-2-endpoint",
            "host_port": 5007,
            "container_port": 8080,
            "expose": 30007
          }
        ]
      },
      "node_name": "k8s-node-2",
      "t_proc": 2,
      "hw_reqs": 1,
      "iot_reqs": [
        "F0:8A:D5:18:88:99"
      ],
      "sec_reqs": []
    }
  ],
  "flows": [
    {
      "src": "frontend",
      "dst": "api-gateway",
      "bandwidth": 1
    },
    {
      "src": "api-gateway",
      "dst": "device-ms",
      "bandwidth": 1
    },
    {
      "src": "device-ms",
      "dst": "device-driver-1",
      "bandwidth": 1
    },
    {
      "src": "device-ms",
      "dst": "device-driver-2",
      "bandwidth": 1
    }
  ],
  "max_latency": [
    {
      "chain": [
        "frontend",
        "api-gateway",
        "device-ms",
        "device-driver-1"
      ],
      "value": 99999
    },
    {
      "chain": [
        "frontend",
        "api-gateway",
        "device-ms",
        "device-driver-2"
      ],
      "value": 99999
    }
  ]
}